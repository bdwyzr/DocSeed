###########
# Muximux #
###########

   muximux:
    restart: always
    image: linuxserver/muximux
    container_name: tool-manage_Muximux
    volumes:
      - CONF/muximux/conf:/config:rw
      - /etc/localtime:/etc/localtime:ro
    environment:
      - PUID=1069
      - PGID=1069
      - TZ=Europe/Paris
    ports:
      - "80:80"

