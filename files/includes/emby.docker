########
# Emby #
########

   emby:
    image: emby/embyserver
    container_name: stream-video_emby
    restart: always
    hostname: video
    volumes:
      - MOVIES:/data:ro
      - CONF/emby/conf:/config:rw
      - /etc/localtime:/etc/localtime:ro
    environment:
      - APP_UID=1069
      - APP_GID=1069
    ports:
      - "5009:8096"
      - "8920:8920"
      - "7359:7359/udp"
      - "1900:1900/udp"

