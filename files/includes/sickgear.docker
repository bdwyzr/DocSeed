############
# SickGear #
############

   sickgear:
    image: ressu/sickgear
    container_name: autodl-tv_sickgear
    hostname: sickgear
    volumes:
      - CONF/sickgear:/data
      - INCOMING:/incoming
      - TVSHOWS:/tv
      - /etc/localtime:/etc/localtime:ro
    restart: always
    environment:
      - APP_UID=1069
      - APP_GID=1069
      - TZ=Europe/Paris
    ports:
      - 5005:8081

