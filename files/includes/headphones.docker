##############
# Headphones #
##############

   headphones:
    image: linuxserver/headphones
    container_name: autodl-music_headphones
    hostname: headphones
    volumes:
      - CONF/headphones:/config
      - INCOMING:/downloads
      - ZIC:/music
      - /etc/localtime:/etc/localtime:ro
    restart: always
    environment:
      - PUID=1069
      - PGID=1069
      - TZ=Europe/Paris
    ports:
      - 5007:8181

