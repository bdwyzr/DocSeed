#########
# Mylar #
#########

   mylar:
    image: linuxserver/mylar
    container_name: autodl-comics_mylar
    hostname: mylar
    volumes:
      - CONF/mylar:/config
      - INCOMING:/downloads
      - BDS:/comics
      - /etc/localtime:/etc/localtime:ro
    restart: always
    environment:
      - PUID=1069
      - PGID=1069
      - TZ=Europe/Paris
    ports:
      - 5006:8090

